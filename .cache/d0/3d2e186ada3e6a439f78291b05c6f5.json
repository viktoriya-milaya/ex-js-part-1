___________ */\r\n\r\n.first-part {\r\n    position: relative;\r\n}\r\n\r\n\r\n.brief {\r\n    position: absolute;\r\n    bottom: 60px;\r\n    right: 60px;\r\n    z-index: 1;\r\n\r\n    font-family: 'Delicious Handrawn';\r\n    font-size: 36px;\r\n    color: #ffffff;\r\n\r\n    text-shadow: 0 0 5px #ffffff, 0 0 10px #ffffff, 0 0 20px #ffffff, 0 0 40px #ff00de, 0 0 80px #ff00de, 0 0 90px #ff00de, 0 0 100px #ff00de, 0 0 150px #ff00de;\r\n}\r\n\r\n\r\n.slider {\r\n    width: 100%;\r\n    height: 600px;\r\n\r\n    overflow: hidden;\r\n    position: relative;\r\n}\r\n\r\n\r\n.slider-list {\r\n    width: 100%;\r\n\r\n    display: flex;\r\n    align-items: center;\r\n    transition: 0.3s ease-in-out;\r\n\r\n    z-index: -1;\r\n}\r\n\r\n\r\n.slider-list__img {\r\n    width: 100%;\r\n    height: 600px;\r\n    object-fit: cover;\r\n}\r\n\r\n\r\n/* arrow */\r\n\r\n.btns {\r\n    display: flex;\r\n    justify-content: space-between;\r\n\r\n}\r\n\r\n\r\n.btn {\r\n    position: absolute;\r\n    top: calc(50% - 16px);\r\n\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n\r\n    width: 32px;\r\n    height: 32px;\r\n    padding: 4px;\r\n    background: none;\r\n    border: #6600cc 1px solid;\r\n    border-radius: 50%;\r\n\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n.btn path {\r\n    fill: #6600cc;\r\n}\r\n\r\n\r\n.btn:hover path {\r\n    fill: inherit;\r\n}\r\n\r\n\r\n.btn:hover {\r\n    background: #6600cc;\r\n}\r\n\r\n\r\n.btn:disabled {\r\n    opacity: 0;\r\n}\r\n\r\n\r\n.btn_prev {\r\n    left: 0;\r\n    transform: scale(-1, 1);\r\n}\r\n\r\n\r\n.btn_next {\r\n    right: 0;\r\n}\r\n\r\n\r\n\r\n/* pagination */\r\n\r\n.slider__dot {\r\n    display: flex;\r\n    justify-content: center;\r\n    gap: 10px;\r\n\r\n    padding: 10px 0;\r\n}\r\n\r\n\r\n.dot {\r\n    width: 36px;\r\n    height: 1px;\r\n    padding: 1px;\r\n    border: inherit 1px solid;\r\n    background: none;\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n.dot_active {\r\n    background-color: #6600cc;\r\n    box-shadow: 0 0 5px #ffffff, 0 0 10px #ffffff, 0 0 20px #ffffff, 0 0 40px #ff00de;\r\n\r\n}\r\n\r\n\r\n.dot:hover {\r\n    box-shadow: 0 0 5px #ffffff, 0 0 10px #ffffff, 0 0 20px #ffffff, 0 0 40px #ff00de;\r\n}\r\n\r\n\r\n/* _____________CAROUSEL________________________ */\r\n\r\n\r\n.second-part {\r\n    margin-top: 100px;\r\n}\r\n\r\n\r\n.carousel {\r\n    position: relative;\r\n    max-width: 80%;\r\n\r\n    padding: 40px 0;\r\n    margin: 0 auto;\r\n}\r\n\r\n\r\n.carousel-wrap {\r\n    overflow: hidden;\r\n    max-width: 880px;\r\n\r\n    margin: 0 auto;\r\n\r\n}\r\n\r\n\r\n.carousel-list {\r\n    height: 160px;\r\n    display: flex;\r\n    gap: 20px;\r\n\r\n    position: relative;\r\n}\r\n\r\n\r\n.carousel-list__img {\r\n    width: 160px;\r\n    height: 160px;\r\n    object-fit: cover;\r\n\r\n    display: block;\r\n}\r\n\r\n/* _____________GALLERY________________________ */\r\n\r\n\r\n.gallery-list {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    gap: 20px;\r\n}\r\n\r\n.gallery-list__item {\r\n    display: flex;\r\n    flex-grow: 1;\r\n    flex: 0 0 calc(20% - (80px / 5));\r\n    flex-direction: column;\r\n    align-items: center;\r\n\r\n    padding-top: 10px;\r\n}\r\n\r\n\r\n.gallery-list__img {\r\n    height: 240px;\r\n    width: 240px;\r\n    object-fit: cover;\r\n\r\n    cursor: pointer;\r\n}\r\n\r\n.gallery-list__img:hover {\r\n    opacity: 0.2;\r\n}\r\n\r\n.title {\r\n    font-size: 16px;\r\n    padding: 10px 0;\r\n}\r\n\r\n\r\n/* modal */\r\n\r\n.modal {\r\n    display: none;\r\n    position: fixed;\r\n    inset: 0;\r\n    opacity: 0;\r\n    visibility: hidden;\r\n    transition: opacity 0.3s ease-in-out;\r\n\r\n    z-index: 1;\r\n\r\n    cursor: pointer;\r\n\r\n    background-color: rgba(0, 0, 0, 0.88);\r\n\r\n}\r\n\r\n.modal__content {\r\n    position: relative;\r\n    max-width: 600px;\r\n    height: 600px;\r\n    padding: 60px;\r\n    margin: auto;\r\n\r\n    cursor: default;\r\n}\r\n\r\n\r\n.modal__img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n\r\n    position: relative;\r\n    margin: auto;\r\n}\r\n\r\n.close {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 16px;\r\n\r\n    border: none;\r\n    background-color: transparent;\r\n    color: beige;\r\n    font-size: 36px;\r\n    padding: 4px;\r\n\r\n    cursor: pointer;\r\n}\r\n\r\n.modal__title {\r\n    text-align: center;\r\n}\r\n\r\n\r\n/* footer */\r\n\r\n.footer {\r\n    padding: 60px;\r\n    margin-left: 0;\r\n}\r\n\r\n\r\n.footer__link {\r\n    color: inherit;\r\n    font-size: 14px;\r\n}\r\n\r\n\r\n.footer__link:hover {\r\n    color: blueviolet;\r\n\r\n}\r\n\r\n\r\n.footer__link svg {\r\n    fill: beige;\r\n}"},"lineCount":363}},"error":null,"hash":"20b1d1536fe6874d4de41c1f6400f50d","cacheData":{"env":{}}}